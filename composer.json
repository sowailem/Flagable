{
  "name": "sowailem/flagable",
  "description": "A Laravel package to add flagging functionality to models",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Abdullah Sowailem",
      "email": "abdullah.sowailem@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Sowailem\\Flagable\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sowailem\\Flagable\\FlagableServiceProvider"
      ],
      "aliases": {
        "Flag": "Sowailem\\Flagable\\Facades\\Flag"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}